# Background Colors
YELLO='\033[48;2;252;244;52m'
WHITE='\033[48;2;255;255;255m'
PURPL='\033[48;2;156;89;209m'
BLACK='\033[48;2;44;44;44m'

# No Color
NC='\033[0m'

# Print the flag
printf "\n"
printf "${YELLO}                          ${NC}\n"
printf "${WHITE}                          ${NC}\n"
printf "${PURPL}                          ${NC}\n"
printf "${BLACK}                          ${NC}\n"
printf "\n"
